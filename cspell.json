{
  "version": "0.2",
  "language": "en,ru",
  "import": [
    "@cspell/dict-ru_ru/cspell-ext.json"
  ],
  "words": [
    "docusaurus",
    "mdx",
    "clsx",
    "prism",
    "swizzle",
    "browserslist",
    "op",
    "cspell",
    "markdownlint"
  ],
  "ignorePaths": [
    "node_modules/**",
    "build/**",
    ".docusaurus/**",
    "package-lock.json",
    "*.log"
  ],
  "dictionaryDefinitions": [
    {
      "name": "custom",
      "path": "./cspell-custom-words.txt",
      "description": "Custom words dictionary",
      "addWords": true
    }
  ],
  "dictionaries": [
    "custom"
  ],
  "useGitignore": true
}
